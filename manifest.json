{
	"manifest_version": 2,
	"name": "Weathery",
	"version": "1.0",
	"default_locale": "en",
	"description": "Weather App created by Ish Kapoor",
	"icons": {"216":"icon.png"},
	"browser_action": {
		"default_icon" : "icon.png",
		"default_popup" : "index.html"
	},
	"page_action": {},
	"action": "",
	"author": "",
	"automation": "",
	"background": {
		// "persistent": false,
		// "service_worker": "",
		"scripts": ["script.js"]
	},
	"externally_connectable": {
		"matches": [
			"*://*.example.com/*"
		]
	},
	"file_browser_handlers": [""
	],
	"file_system_provider_capabilities": {
		"configurable": true,
		"multiple_mounts": true,
		"source": "network"
	},
	"homepage_url": "https://WeatherApp.ishkapoor.repl.co",
	"host_permissions": "",
	"import": [
		{
			"id": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
		}
	],
	"incognito": "spanning",
	"input_components": "",
	"key": "publicKey",
	"minimum_chrome_version": "89.0.4389",
	"offline_enabled": true,
	"omnibox": {
		"keyword": "aString"
	},
	"optional_permissions": [
		"tabs"
	],
	"options_page": "options.html",
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	],
	"short_name": "Weathery",
	"storage": {
		"managed_schema": "schema.json"
	},
	"system_indicator": "",
	"tts_engine": {"":""
	},
	"update_url": "https://WeatherApp.ishkapoor.repl.co",
	"version_name": "",
	"web_accessible_resources": [""
	]
}